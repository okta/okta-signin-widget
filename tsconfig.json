{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "checkJs": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "noEmit": true,
    "outDir": "./types",
    "paths": {
      "config/*": [
        "./src/config/*"
      ],
      "duo": [
        "./packages/vendor/duo_web_sdk/index.js"
      ],
      "exports/*": [
        "./src/exports/*"
      ],
      "handlebars-inline-precompile": [
        "./scripts/buildtools/tsc/handlebars-inline-precompile"
      ],
      "models/*": [
        "./src/models/*"
      ],
      "nls/*": [
        "./packages/@okta/i18n/src/json/*"
      ],
      "okta": [
        "./packages/@okta/courage-dist/types"
      ],
      "okta-i18n-bundles": [
        "./src/util/Bundles.ts"
      ],
      "qtip": [
        "./packages/@okta/qtip2/dist/jquery.qtip.js"
      ],
      "src/*": [
        "./src/v3/src/*"
      ],
      "types": [
        "./src/types"
      ],
      "types/*": [
        "./src/types/*"
      ],
      "typingdna": [
        "./packages/vendor/TypingDnaRecorder-JavaScript/typingdna"
      ],
      "util/*": [
        "./src/util/*"
      ],
      "v1/*": [
        "./src/v1/*"
      ],
      "v2/*": [
        "./src/v2/*"
      ],
      "views/*": [
        "./src/views/*"
      ],
      "widget/*": [
        "./src/widget/*"
      ]
    },
    "plugins": [
      {
        "transform": "@zerollup/ts-transform-paths"
      }
    ],
    "resolveJsonModule": true,
    "rootDir": ".",
    "sourceMap": true,
    "target": "es6",
    "skipLibCheck": true
  },
  "exclude": [
    "types/generated/*"
  ],
  "files": [
    "src/exports/default.ts"
  ],
  "include": [
    "src/types/*",
    "src/exports/*",
    "src/authClient/*",
    "src/router/*"
  ]
}
