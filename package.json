{
  "name": "@okta/okta-signin-widget",
  "description": "The Okta Sign-In Widget",
  "version": "2.9.0",
  "homepage": "https://github.com/okta/okta-signin-widget",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/okta/okta-signin-widget.git"
  },
  "bugs": {
    "url": "https://github.com/okta/okta-signin-widget/issues"
  },
  "files": [
    "dist"
  ],
  "browserslist": [
    "> 0.1%",
    "not ie <= 9"
  ],
  "main": "./dist/js/okta-sign-in.entry.js",
  "scripts": {
    "clean": "rm -rf target && rm -rf dist",
    "lint:report": "grunt lint --checkstyle",
    "lint": "grunt lint",
    "test": "grunt test",
    "install:react": "npm install --prefix test/e2e/react-app",
    "prestart:react": "kill -9 $(lsof -t -i:3001 -sTCP:LISTEN) || true",
    "start:react": "npm start --prefix test/e2e/react-app & wait-on http-get://localhost:3001",
    "install:angular": "npm install --prefix test/e2e/angular-app",
    "prestart:angular": "kill -9 $(lsof -t -i:4200 -sTCP:LISTEN) || true",
    "start:angular": "npm start --prefix test/e2e/angular-app & wait-on http-get://localhost:4200",
    "pretest:e2e": "npm run install:react && npm run install:angular && node pretest.js",
    "test:e2e": "npm run build:release && npm run start:react && npm run start:angular && grunt test-e2e",
    "protractor": "webdriver-manager update && protractor target/e2e/conf.js",
    "build:dev": "grunt build:dev",
    "build:release": "grunt build:release",
    "build:webpack-dev": "webpack --config webpack.dev.config.js",
    "build:webpack-release": "webpack --config webpack.release.config.js",
    "build:webpack-test": "webpack --config webpack.test.config.js",
    "build:webpack-e2e-app": "webpack target/e2e/app/app-using-osw-entry.js target/js/app-bundle.js",
    "build:webpack-e2e-dev-app": "webpack target/e2e/app/app-using-osw-dev.js target/js/app-bundle.js",
    "prestart": "npm run build:dev",
    "start": "grunt start-server-open",
    "prepublish": "npm run build:release"
  },
  "devDependencies": {
    "app-root-path": "1.0.0",
    "autoprefixer": "6.7.7",
    "axe-core": "2.0.7",
    "axe-webdriverjs": "0.4.0",
    "babel-core": "6",
    "babel-loader": "6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "cssnano": "3.10.0",
    "fs-extra": "1.0.0",
    "git-rev-sync": "1.4.0",
    "grunt": "0.4.5",
    "grunt-cli": "0.1.13",
    "grunt-contrib-connect": "0.11.2",
    "grunt-contrib-copy": "0.8.0",
    "grunt-contrib-jasmine": "1.1.0",
    "grunt-contrib-jshint": "0.11.2",
    "grunt-contrib-requirejs": "0.4.4",
    "grunt-contrib-watch": "1.0.0",
    "grunt-eslint": "^19.0.0",
    "grunt-exec": "0.4.6",
    "grunt-json-generator": "0.1.0",
    "grunt-postcss": "0.8.0",
    "grunt-retire": "0.3.12",
    "grunt-sass": "2.0.0",
    "grunt-search": "^0.1.8",
    "grunt-template-jasmine-requirejs": "0.2.3",
    "imports-loader": "0.6.5",
    "json-loader": "0.5.4",
    "junit-report-builder": "1.1.1",
    "load-grunt-tasks": "3.5.2",
    "moment": "2.12.0",
    "normalize-url": "1.4.1",
    "null-loader": "0.1.1",
    "open": "0.0.5",
    "phantomjs": "1.9.20",
    "protractor": "5.2.2",
    "request-promise": "2.0.0",
    "semver": "5.1.0",
    "time-grunt": "1.4.0",
    "wait-on": "2.0.2",
    "webdriver-manager": "12.0.6",
    "webpack": "1.13.1"
  },
  "dependencies": {
    "@okta/okta-auth-js": "1.17.0",
    "babel-polyfill": "^6.23.0",
    "babel-runtime": "^6.26.0",
    "backbone": "1.2.1",
    "clipboard": "1.6.1",
    "handlebars": "4.0.5",
    "jquery": "1.12.1",
    "jquery-placeholder": "2.0.7",
    "jquery.cookie": "1.4.1",
    "q": "1.4.1",
    "u2f-api-polyfill": "0.4.1",
    "underscore": "1.8.3"
  },
  "optionalDependencies": {
    "fsevents": "*"
  }
}
