    "stylelint": "^10.1.0",
    "stylelint-checkstyle-formatter": "^0.1.2",
    "stylelint-scss": "^3.9.2",
    "testcafe": "^1.4.0",
    "time-grunt": "^2.0.0",
    "wait-on": "^3.3.0",
    "webdriver-manager": "^12.1.8",
    "webpack": "^3.5.4",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-dev-server": "2.11.3"
  },
  "dependencies": {
