{
  "compilerOptions": {
    "noEmit": true,
    "baseUrl": ".",
    "outDir": "./target/types",
    "sourceMap": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "checkJs": false,
    "skipLibCheck": true,
    "target": "ES6",
    "paths": {
      "vendor/*": ["./src/courage/vendor/*"],
      "handlebars-inline-precompile": ["./src/util/handlebars-inline-precompile"],
      "okta-i18n-bundles": ["./src/courage/okta-i18n-bundles.js"],
      "moment": ["./src/courage/vendor/lib/moment-with-locales"]
    }
  },
  "include": [
    "src/types/**/*"
  ],
  "files": [
    "src/index.ts"
  ],
  "types": []
}
